{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  },
  "Cookidoo": {
    "BaseUrl": "https://cookidoo.thermomix.com",
    "ApiVersion": "v1",
    "Timeout": "00:01:00",
    "MaxRetryAttempts": 1,
    "RetryDelay": "00:00:01",
    "UserAgent": "Cookidoo-MCP-Server-Dev/1.0"
  },
  "Jwt": {
    "SecretKey": "DevelopmentSecretKeyForJwtTokenGeneration123456789",
    "Issuer": "CookidooMcpServer",
    "Audience": "CookidooMcpApi",
    "ExpirationMinutes": 120
  }
} 